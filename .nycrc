{
  "include": [
    "static/**/*.js"
  ],
  "exclude": [
    "static/**/*.spec.js",
    "static/**/*.test.js",
    "static/third_party/**",
    "test/**",
    "build/**",
    "dist/**",
    "node_modules/**"
  ],
  "reporter": [
    "html",
    "text",
    "lcov",
    "json-summary"
  ],
  "check-coverage": false,
  "lines": 60,
  "functions": 60,
  "branches": 50,
  "statements": 60,
  "all": true,
  "report-dir": "./coverage",
  "temp-dir": "./.nyc_output",
  "exclude-after-remap": true
}
