{
  "name": "wheel-spinner",
  "version": "1.0.0",
  "description": "This README explains how to install the app and get it running. If you don't want to install it yourself, you can always use the copy running at [wheelofnames.com](https://wheelofnames.com).",
  "private": true,
  "dependencies": {
    "@babel/polyfill": "^7.11.5",
    "better-sqlite3": "^11.0.0",
    "buefy": "^0.8.20",
    "canvas-confetti": "^1.2.0",
    "circletype": "^2.3.0",
    "core-js": "^3.20.2",
    "cors": "^2.8.5",
    "express": "^4.21.0",
    "howler": "^2.2.0",
    "node-vibrant": "^3.1.5",
    "regenerator-runtime": "^0.13.9",
    "vue": "^2.6.12",
    "vue-i18n": "^8.21.0",
    "vue-mq": "^1.0.1",
    "vue-router": "^3.5.2",
    "vuex": "^3.5.1",
    "workbox-webpack-plugin": "^6.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/eslint-parser": "^7.28.6",
    "@babel/preset-env": "^7.11.5",
    "@babel/register": "^7.11.5",
    "async-css-plugin": "^1.1.5",
    "babel-loader": "^8.1.0",
    "cloc": "^2.6.0",
    "copy-webpack-plugin": "^6.1.0",
    "css-loader": "^4.2.2",
    "dotenv-webpack": "^2.0.0",
    "eslint": "^8.57.1",
    "eslint-plugin-vue": "^8.7.1",
    "file-loader": "^6.1.0",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^0.11.0",
    "mocha": "^8.1.3",
    "nyc": "^17.1.0",
    "style-loader": "^1.2.1",
    "vue-eslint-parser": "^9.4.3",
    "vue-loader": "^15.9.3",
    "vue-template-compiler": "^2.6.12",
    "webpack": "^5.60.0",
    "webpack-cli": "^4.9.1",
    "webpack-merge": "^5.1.3"
  },
  "scripts": {
    "start": "node backend/server.js",
    "start:dev": "npm run build:dev && npm run start",
    "clean": "rm -rf dist/*",
    "build:dev": "npm run clean && NODE_OPTIONS=--openssl-legacy-provider webpack --config build/dev.config.js",
    "build:test": "npm run clean && NODE_OPTIONS=--openssl-legacy-provider webpack --config build/test.config.js",
    "build:prod": "npm run clean && NODE_OPTIONS=--openssl-legacy-provider webpack --config build/prod.config.js",
    "test": "mocha --require @babel/register",
    "test:debug": "mocha debug --require @babel/register",
    "test:coverage": "nyc npm test",
    "test:coverage:report": "nyc report --reporter=text-lcov > coverage/lcov.info",
    "lint": "eslint --ext .js,.vue static/ test/ backend/",
    "lint:fix": "eslint --ext .js,.vue static/ test/ backend/ --fix",
    "generate:stats": "webpack --config build/prod.config.js --profile --json > webpack-stats.json",
    "analyze:stats": "open https://chrisbateman.github.io/webpack-visualizer",
    "cloc": "npx cloc --vcs=git --exclude-ext=json,xml --exclude-dir=old_bundles"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/momander/wheel-spinner.git"
  },
  "keywords": [],
  "author": "",
  "license": "Apache-2.0",
  "homepage": "https://github.com/momander/wheel-spinner#readme"
}
